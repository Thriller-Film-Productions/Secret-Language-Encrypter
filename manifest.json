{
  "manifest_version": 2,

  "name": "Decrypter/Encrypter",
  "description": "This extension lets people encrypt and decrypt messages",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": [
        "*://www.anonychat.ga/*"
      ],
      "js":["main.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
